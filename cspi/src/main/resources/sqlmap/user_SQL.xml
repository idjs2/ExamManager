<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="kr.or.cspi.mapper.UserMapper">
    <select id="authenticate" resultType="userVO">
        SELECT *
        FROM member
        WHERE mem_id = #{memId}
        AND mem_pw = #{memPw}
    </select>
</mapper>
