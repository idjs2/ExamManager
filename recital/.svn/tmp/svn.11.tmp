<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.or.ddit.mapper.ILectureTeamMapper">
	<insert id="insertTeam" parameterType="teamVO">
		<selectKey keyProperty="teaNo" resultType="String" order="BEFORE">
			select 'TEA_'||SEQ_TEAM.nextval from dual
		</selectKey>
		insert into team 
		values(#{teaNo},#{lecNo},#{teaName})
	</insert>
	<insert id="insertMember" parameterType="teamVO">
		insert into team_member
		values(#{teaNo},#{stuNo})
	</insert>
</mapper>


























