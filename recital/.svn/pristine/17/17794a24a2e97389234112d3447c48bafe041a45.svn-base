<%@ page language="java" contentType="text/html; charset=UTF-8" pageEncoding="UTF-8"%>
<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <title>맛집 게시판 글 작성</title>
</head>
<body>
    <h2>맛집 게시판 글 작성</h2>
    <form action="${pageContext.request.contextPath}/student/foodWrite" method="post">
        <table border="1">
            <tr>
                <th>제목</th>
                <td><input type="text" name="food_title" /></td>
            </tr>
            <tr>
                <th>작성자</th>
                <td><input type="text" name="food_writer" /></td>
            </tr>
            <tr>
                <th>내용</th>
                <td><textarea name="food_content"></textarea></td>
            </tr>
            <tr>
                <th>지도 좌표 X</th>
                <td><input type="text" name="food_map_x" /></td>
            </tr>
            <tr>
                <th>지도 좌표 Y</th>
                <td><input type="text" name="food_map_y" /></td>
            </tr>
        </table>
        <button type="submit">등록</button>
    </form>
</body>
</html>
